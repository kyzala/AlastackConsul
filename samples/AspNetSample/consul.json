{
  "Consul": {
    "Agent": {
      "Datacenter": "dc1",
      "Address": "http://127.0.0.1:8500",
      //"Token": "045ee828-2fa4-45b7-b01d-d9fb3937da54",
      "WaitTime": "00:00:05"
    },
    "Configuration": {
      "PathBase": "alastack/aspnetsample",
      "Namespace": "ns.default",
      "Sets": [
        {
          "Id": "logging.json",
          "Group": "gp.default"
        },
        {
          "Id": "config1.json",
          "Group": "gp.group1"
        },
        {
          "Id": "config2.json",
          "Group": "gp.group1",
          "Optional": false,
          "ReloadOnChange": false,
          "PollingWaitTime": "00:00:10",
          "IgnoreException": true
        }
      ],
      "Metadata": {
        "MyKey1": "MyValue1"
      }
    },
    "Registration": {
      //"Id": "aspnetsample1",
      "Name": "AspNetSample",
      "Version": "1.0.0",
      "Address": "http://127.0.0.1:5000",
      "Tags": [ "apiserver", "test" ],
      "EnableTagOverride": true,
      "Metadata": {
        "RegistrationIdPolicy": "Default", // Default, ByConsul
        "HealthCheckIdPolicy": "Default", // Default, ByConsul
        "HealthCheckNamePolicy": "Default", // Default, ByConsul
        "MyKey1": "MyValue1"
      },
      "HealthCheck": {
        //"CheckId": null,
        //"Name": "AspNetSample_HealthCheck",
        "DeregisterCriticalServiceAfter": "00:01:00",
        "Interval": "00:00:15",
        "Health": "http://127.0.0.1:5000/health",
        "Timeout": "00:00:10"
      }
    }
  }
}